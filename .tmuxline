# This tmux statusbar config was created by tmuxline.vim
# on Mon, 02 Nov 2020

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#c6c6c6,bg=#303030"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#80a0ff"
set -g status-style "none,bg=#303030"
set -g message-style "fg=#c6c6c6,bg=#303030"
set -g pane-border-style "fg=#303030"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none,fg=#80a0ff,bg=#303030"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#c6c6c6,bg=#303030"
set -g status-left "#[fg=#1c1c1c,bg=#80a0ff] [#S] #[fg=#80a0ff,bg=#303030,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#303030,bg=#303030,nobold,nounderscore,noitalics]#[fg=#c6c6c6,bg=#303030] #($TMUX_PLUGIN_MANAGER_PATH/tmux-mem-cpu-load/tmux-mem-cpu-load -i 2 -a 0) #[fg=#303030,bg=#303030,nobold,nounderscore,noitalics]#[fg=#c6c6c6,bg=#303030] %m/%d %H:%M:%S #[fg=#80a0ff,bg=#303030,nobold,nounderscore,noitalics]#[fg=#1c1c1c,bg=#80a0ff] #H "
setw -g window-status-format "#[fg=#303030,bg=#303030,nobold,nounderscore,noitalics]#[default] #I:#W#F #[fg=#303030,bg=#303030,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=#303030,bg=#303030,nobold,nounderscore,noitalics]#[fg=#c6c6c6,bg=#303030] #I:#W#F #[fg=#303030,bg=#303030,nobold,nounderscore,noitalics]"
