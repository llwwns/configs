# This tmux statusbar config was created by tmuxline.vim
# on Tue, 10 Mar 2020

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#599cab,bg=#091f2e"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#195466"
set -g status-style "none,bg=#11151c"
set -g message-style "fg=#599cab,bg=#091f2e"
set -g pane-border-style "fg=#091f2e"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "underscore,fg=#195466,bg=#11151c"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#195466,bg=#11151c"
set -g status-left "#[fg=#99d1ce,bg=#195466] [#S] #[fg=#195466,bg=#11151c,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#11151c,bg=#11151c,nobold,nounderscore,noitalics]#[fg=#195466,bg=#11151c] #($TMUX_PLUGIN_MANAGER_PATH/tmux-mem-cpu-load/tmux-mem-cpu-load -i 2 -a 0) #[fg=#091f2e,bg=#11151c,nobold,nounderscore,noitalics]#[fg=#599cab,bg=#091f2e] %m/%d %H:%M:%S #[fg=#195466,bg=#091f2e,nobold,nounderscore,noitalics]#[fg=#99d1ce,bg=#195466] #H "
setw -g window-status-format "#[fg=#11151c,bg=#11151c,nobold,nounderscore,noitalics]#[default] #I:#W#F #[fg=#11151c,bg=#11151c,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=#11151c,bg=#091f2e,nobold,nounderscore,noitalics]#[fg=#599cab,bg=#091f2e] #I:#W#F #[fg=#091f2e,bg=#11151c,nobold,nounderscore,noitalics]"
